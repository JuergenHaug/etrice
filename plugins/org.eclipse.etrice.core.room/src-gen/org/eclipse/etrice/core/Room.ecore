      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="&lt;br>The base class from which all messages are inherited.&#xA;The port classes and the semantics are not inherited.&#xA;"/>
      </eAnnotations>
    </eStructuralFeatures>
