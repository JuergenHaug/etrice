<templates>

	<template autoinsert="true" context="org.eclipse.etrice.core.Room.RoomModel"
	deleted="false" description="model skeleton" enabled="true"
	id="model" name="RoomModel">RoomModel ${ModelName} {&#13;
	LogicalSystem ${LogSysName} {&#13;
