		}
		
		// argument of PrimitiveType, no return value
		Operation operation2(Param1: int32): void {
			"UserCodeLine1"
		}
		
		// argument of PrimitiveType, return value of of PrimitiveType
		Operation operation3(Param1: int32): float64 {
